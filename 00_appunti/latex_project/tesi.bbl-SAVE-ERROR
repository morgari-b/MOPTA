      \strng{fullhash}{3e177821bd8f8c9fa58c98e08cfbcedd}
      \strng{fullhashraw}{3e177821bd8f8c9fa58c98e08cfbcedd}
      \strng{bibnamehash}{3e177821bd8f8c9fa58c98e08cfbcedd}
      \strng{authorbibnamehash}{3e177821bd8f8c9fa58c98e08cfbcedd}
      \strng{authornamehash}{fe83fc331f6fdbbc9f95ac1268a71cbb}
      \strng{authorfullhash}{3e177821bd8f8c9fa58c98e08cfbcedd}
