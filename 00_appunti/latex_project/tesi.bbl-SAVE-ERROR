      \strng{namehash}{bcfa84887c462c38f13b898edb39c0e9}
      \strng{fullhash}{bcfa84887c462c38f13b898edb39c0e9}
      \strng{fullhashraw}{bcfa84887c462c38f13b898edb39c0e9}
